Description='A sample netctl connection with MSCHAP'
Interface=wlp2s0
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
    'ssid="UP-Campus"'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'pairwise=TKIP CCMP'
    'identity="xxx@uni-potsdam.de"'
    'password="xxx"'
    'priority=1'
    'phase2="auth=MSCHAPV2"'
)
