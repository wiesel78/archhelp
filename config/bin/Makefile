BINDIR = bin
BINDIRPHIL = $(BINDIR)
BINDIRHENRIK = $(BINDIR)


all:

pull-phil:
	mkdir -p ${HOME}/$(BINDIRPHIL)/
	cp -r _phil/* ${HOME}/$(BINDIRPHIL)/

push-phil:
	cp -r ${HOME}/$(BINDIRPHIL)/* _phil/


pull-henrik:
	mkdir -p ${HOME}/$(BINDIRHENRIK)/
	cp _henrik/* ${HOME}/$(BINDIRHENRIK)/

push-henrik:
	cp -r ${HOME}/$(BINDIRHENRIK)/* _henrik/

